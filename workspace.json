{
  "version": 1,
  "projects": {
    "list-api": {
      "projectType": "application",
      "root": "apps/list-api",
      "sourceRoot": "apps/list-api/src",
      "architect": {
        "build": {
          "builder": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/list-api",
            "main": "apps/list-api/src/main.go"
          }
        },
        "serve": {
          "builder": "@nx-go/nx-go:serve",
          "options": {
            "cmd": "gow",
            "main": "apps/list-api/src/main.go"
          }
        },
        "test": {
          "builder": "@nx-go/nx-go:test",
          "options": {
            "main": "apps/list-api/src/main_test.go"
          }
        },
        "lint": {
          "builder": "@nx-go/nx-go:lint"
        }
      }
    },
    "main-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/main-frontend",
      "sourceRoot": "apps/main-frontend/src",
      "prefix": "go-list",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/main-frontend",
            "index": "apps/main-frontend/src/index.html",
            "main": "apps/main-frontend/src/main.ts",
            "polyfills": "apps/main-frontend/src/polyfills.ts",
            "tsConfig": "apps/main-frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/main-frontend/src/favicon.ico",
              "apps/main-frontend/src/assets"
            ],
            "styles": ["apps/main-frontend/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/main-frontend/src/environments/environment.ts",
                  "with": "apps/main-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "main-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "main-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main-frontend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/main-frontend/tsconfig.app.json",
              "apps/main-frontend/tsconfig.spec.json",
              "apps/main-frontend/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/main-frontend/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/main-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-frontend-e2e": {
      "root": "apps/main-frontend-e2e",
      "sourceRoot": "apps/main-frontend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/main-frontend-e2e/cypress.json",
            "tsConfig": "apps/main-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "main-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/main-frontend-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/main-frontend-e2e/**/*"]
          }
        }
      }
    },
    "geo": {
      "projectType": "library",
      "root": "libs/geo",
      "sourceRoot": "libs/geo/src",
      "prefix": "go-list",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/geo/tsconfig.lib.json",
              "libs/geo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/geo/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/geo/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
